        android:text="Dias Sin Pegar \n a nadie"
        app:layout_constraintEnd_toStartOf="@+id/ut401FragmentPegar"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ut401idTvNoFumado2" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/ut401FragmentPegar"
        android:name="com.example.pmdm_2324.ut04_Fragments.ut401FragmentBuenaPersona"
        android:layout_width="173dp"
        android:layout_height="173dp"
        android:layout_marginEnd="40dp"
