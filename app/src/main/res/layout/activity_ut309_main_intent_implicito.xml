<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ut03_PasosDeDatosEntreActividades.ut309MainIntentImplicito">

    <Button
        android:id="@+id/ut309btEleccion"
        android:layout_width="193dp"
        android:layout_height="81dp"
        android:layout_marginTop="56dp"
        android:text="Mapa"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ut309btSMS"
        app:layout_constraintVertical_bias="0.223" />

    <Button
        android:id="@+id/ut309btSMS"
        android:layout_width="193dp"
        android:layout_height="81dp"
        android:layout_marginTop="56dp"
        android:text="SMS"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ut309btMarcarNumero" />

    <Button
        android:id="@+id/ut309btMarcarNumero"
        android:layout_width="193dp"
        android:layout_height="81dp"
        android:layout_marginTop="56dp"
        android:text="Marcar Numero"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ut309btCancion" />

    <Button
        android:id="@+id/ut309btCancion"
        android:layout_width="193dp"
        android:layout_height="81dp"
        android:layout_marginTop="56dp"
        android:text="Cancion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>